          "z_seam_position": {"default_value": "left" },
          "dual":{"enabled": "extruders_enabled_count > 0"},
          "prime_tower_brim_enable":{"default_value": true},
          "brim_outside_only":{"default_value": false},
          "prime_tower_size":{"default_value": 30},
